import scrapy


class ForumSpider(scrapy.Spider):
    name = "forum-get-body"

    def start_requests(self):
        url_list = [self.url]
        for url in url_list:
            yield scrapy.Request(url=url, callback=self.parse)


    def parse(self, response):
        print('hey')
